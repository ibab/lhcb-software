<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set><text>../options/test-FSR.py</text>
    <text>--option</text>
    <text>from GaudiConf.Configuration import LHCbApp; LHCbApp().Persistency="POOL"; from Configurables import Gaudi__RootCnvSvc; Gaudi__RootCnvSvc( "RootCnvSvc").OutputLevel = INFO</text>
  </set></argument>
  <argument name="reference"><text>../refs/test-FSR.ref</text></argument>
  <argument name="validator"><text>
from Brunel.QMTest.BrunelExclusions import preprocessor as BrunelPreprocessor
preprocessor = BrunelPreprocessor + \
  LineSkipper(["DEBUG  'CnvServices':"]) + \
  LineSkipper(["ServiceManager      DEBUG"]) + \
  LineSkipper(["VERBOSE ServiceLocatorHelper"]) + \
  LineSkipper(["RootCnvSvc"]) + \
  LineSkipper(["PoolDbBaseCnv"]) + \
  LineSkipper(["PoolRoot"]) + \
  LineSkipper(["POOL"])
validateWithReference(preproc = preprocessor)
</text></argument>
  <argument name="prerequisites"><set>
    <tuple><text>brunel.readdstfsr</text><enumeral>PASS</enumeral></tuple>
  </set></argument>
</extension>
