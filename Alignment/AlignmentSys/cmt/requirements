package AlignmentSys
version           v9r1

branches cmt doc
 
#=======================================================
# LHCb Alignment packages
#=======================================================

# General purpose components

use AlignmentDBVisualisationTool	v1r0	Alignment # Pieter David

use AlignmentInterfaces	v3r2	Alignment # Johan Blouw
use AlignmentTools      v3r0p1  Alignment # Sebastien Viret
use AlignEvent		v1r1    Alignment # Wouter Hulsbergen
use AlignKernel		v3r4    Alignment # Adlene Hicheur, Wouter Hulsbergen
use AlignSolvTools	v3r3    Alignment # A. Hicheur, W. Hulsbergen
use AlignTrTools	v4r11	Alignment # Johan Blouw, Adlene Hicheur, Louis Nicolas
use TAlignment		v5r8	Alignment # Jan Amoraal, Johan Blouw, Marc Deissenroth, Florin Maciuc
#AlignT2Velo Removed, 03.03.2010
#use AlignT2Velo         v1r1    Alignment # Steve Blusk
use VeloAlignment	v4r10p1	Alignment # Marco Gersabeck
use MisAligner		v2r0	Alignment # Johan Blouw, Manuel Schiller
use Escher		v4r14	Alignment # Johan Blouw
use CommonParticles     v8r7    Phys

#=======================================================
# LHCb Calibration packages
#=======================================================

use OTCalibration	v1r5p1  Calibration # Wouter Hulsbergen


# Declare this as a container package
apply_pattern container_package

# Allow the generation of QMTest summary 
apply_pattern QMTestSummarize

#============================================================================
# Install the configurable
apply_pattern install_python_modules
