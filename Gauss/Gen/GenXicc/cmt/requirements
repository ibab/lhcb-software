# =============================================================================
package       GenXicc
version       v2r0

# =========== structure =======================================================
branches      cmt src include doc 

# =========== own includes ====================================================
include_dirs $(GENXICCROOT)/include

# =========== dependencies ====================================================
use      GENSER            v*     Gen
use      GenXiccData       v*

# =========== constituents ====================================================
library GenXicc   *.F

# =========== standard patterns ===============================================
apply_pattern linker_library  library=GenXicc
apply_pattern linker_stamps   library=GenXicc

